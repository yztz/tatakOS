    "version": "0.2.0",
    "configurations": [
        {
            "name": "kernel(gdb)",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/kernel",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            // "logging": {
            //     "engineLogging":"verbose"
            // },
            // "miDebuggerPath": "/opt/riscv-gdb/bin/riscv-gdb",
            "miDebuggerPath": "gdb-multiarch",
            "miDebuggerServerAddress": "localhost:26000",
            "setupCommands": [
                {
                    "description": "riscv",
                    "text": "set architecture riscv:rv64",
                    "ignoreFailures": true
                },
                {
                    "description": "为 gdb 启用整齐打印",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
                {
                    "description": "寄存器显示修复",
                    "text": "set tdesc filename gdb-riscv64.xml"
                },
                // {
                //     "description":  "将反汇编风格设置为 Intel",
                //     "text": "-gdb-set disassembly-flavor intel",
                //     "ignoreFailures": true
                // }
            ],
        },
        {
            "name": "user(gdb)",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/user_prog/thread",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${fileDirname}",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            // "miDebuggerPath": "/opt/kendryte-toolchain/bin/riscv64-unknown-elf-gdb",
            "miDebuggerPath": "gdb-multiarch",
            "miDebuggerServerAddress": "localhost:26000",
            "setupCommands": [
                {
                    "description": "riscv",
                    "text": "set architecture riscv:rv64",
                    "ignoreFailures": true
                },
                {
                    "description": "为 gdb 启用整齐打印",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                },
            ],
        }
    ],
    "compounds": [
        {
            "name": "Compound",
            "configurations": [
                "kernel(gdb)",
                "user(gdb)"
            ]
        }
    ]
}


